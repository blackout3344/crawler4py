{
    "version":1,
    "disable_existing_loggers":false,
    "formatters":{
        "simple":{
            "format":"%(lineno)d  %(asctime)s %(threadName)s--%(filename)s--%(funcName)s--%(levelname)s--%(message)s"
        }
    },
    "handlers":{
        "console":{
            "class":"logging.StreamHandler",
            "level":"DEBUG",
            "formatter":"simple",
            "stream":"ext://sys.stdout"
        }
    },
    "loggers":{
        "py_crawler":{
            "level":"INFO",
            "handlers":["console"],
            "propagate":"no"
        }
    }
}